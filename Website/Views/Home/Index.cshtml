  <script type="text/javascript">
    (function(){
      window.Roblox = window.Roblox || {};
      if (!Roblox.PageHeartbeatEvent) {
        Roblox.PageHeartbeatEvent = {
          Init: function(schedule){
            if (!Array.isArray(schedule) || schedule.length === 0) return;
            var endpoint = '/Roblox/heartbeat';
            var send = function(label){
              var payload = JSON.stringify({ t: Date.now(), l: label || null, p: location.pathname });
              if (navigator.sendBeacon) {
                var blob = new Blob([payload], { type: 'application/json' });
                navigator.sendBeacon(endpoint, blob);
              } else {
                try { fetch(endpoint, { method: 'POST', headers: { 'Content-Type': 'application/json' }, body: payload, keepalive: true }); } catch(e) {}
              }
            };
            for (var i=0;i<schedule.length;i++){
              (function(delay, idx){ setTimeout(function(){ send('t'+delay); }, delay*1000); })(schedule[i], i);
            }
            var last = schedule[schedule.length-1];
            setInterval(function(){ send('interval'+last); }, last*1000);
          }
        };
      }
    })();
  </script>

